            {
              "rule_id": "Textile-Origin-Rule-001",
              "rule_type": null,
              "main_processes": [],
              "thresholds": [
                {
                  "basis": "ex-works price",
                  "comparator": "≤",
                  "limit": 8.0,
                  "unit": "%"
                }
              ],
              "applies_if_any": false,
              "exception_clauses": [
                {
                  "description": "Textile materials, which are not classified within Chapters 50 to 63, may be used freely in the manufacture of textile products, whether or not they contain textiles.",
                  "thresholds": []
                }
              ],
              "commentary": "Non-originating textile materials are considered originating if their HS code at 2-digit level differs from the final product and their value does not exceed 8% of the ex-works price."
            },
